generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model membro {
  id String @id @default(uuid())
  login String?
  senha String?
  nome String?
  motem String?
  telefone String?
  cpf String?
  iniciado Boolean?
  ativo Boolean?
  pagante Boolean?
  email String?
  data_nascimento String?
  grau String?
  createdAt DateTime? @default(now())
}
